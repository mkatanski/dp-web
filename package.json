{
  "name": "dp-web",
  "version": "0.1.0",
  "private": true,
  "homepage": "",
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^10.0.0",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.13",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "dayjs": "^1.8.28",
    "express": "^4.17.1",
    "final-form": "^4.20.0",
    "lodash": "^4.17.15",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-final-form": "^6.5.0",
    "react-redux": "^7.2.0",
    "react-responsive": "^8.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "styled-components": "^5.1.1",
    "typescript": "^3.9.5",
    "validate.js": "^0.13.1"
  },
  "scripts": {
    "start": "BROWSER='none' react-scripts start",
    "build": "react-scripts build",
    "build:server": "true",
    "test": "react-scripts test --all --watchAll=false",
    "test:cover": "react-scripts test --all --watchAll=false --coverage",
    "test:watch": "react-scripts test --all --watchAll",
    "test:watch:cover": "react-scripts test --all --watchAll --coverage",
    "eject": "react-scripts eject",
    "coverage": "istanbul check-coverage --statements 90 --functions 90 --branches 80 --lines 90",
    "reinstall": "rm -rf node_modules && yarn install --frozen-lockfile",
    "reinstall:cache": "rm -rf node_modules && yarn cache clean && yarn install --frozen-lockfile",
    "pretty": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "eslint --config ./.eslintrc --format table 'src/**/*.{ts,tsx}' && tsc --noEmit",
    "open:cover": "open coverage/lcov-report/index.html"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --bail --no-restage && yarn run lint",
      "pre-push": "yarn run lint && yarn run test:cover && yarn run coverage"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/**/index.ts",
      "!src/_devias/**/*.{js,jsx,ts,tsx}",
      "!src/**/__fixtures__/*.{js,jsx,ts,tsx}",
      "!src/serviceWorker.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.3.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/material-ui": "^0.21.7",
    "@types/react-responsive": "^8.0.2",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "istanbul": "^0.4.5",
    "jest-styled-components": "^7.0.2",
    "mockdate": "^3.0.2",
    "mutationobserver-shim": "^0.3.7",
    "nock": "^12.0.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "16.13",
    "redux-devtools": "^3.5.0",
    "whatwg-fetch": "^3.0.0"
  }
}
